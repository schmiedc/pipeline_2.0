#!/bin/bash
/sw/bin/xvfb-run -a /sw/users/schmied/packages/Fiji.app.hdf5/ImageJ-linux64 -Xmx10g -Dprintnumjobs=true 		-Dpath="/projects/pilot_spim/Christopher/2014-06-21_Stock14_Stock19/" -DexportXmlFilename="/hdf5/2014-06-12_Stock14_Stock19.xml " -Dspimresolutions=         	-Dspimsubdivisions= -DinputFilePattern="spim_TL{tt}_Angle{a}_Channel{c}.tif"         	-Dchannels="0" -Dangles="1,2,3,4,5" -Dtimepoints="1-50"         	-DreferenceTimePoint="40" -DoverrideImageZStretching=         	-DzStretching=5.172413 -Dfusionresolutions=         	-Dfusionsubdivisions= -Dfilepath="/output/" -Dfilepattern="%1\$d/DC(l=6.0E-4)_t%1\$d_ch%2\$d_%3\$03d.tif"         	-DnumSlices=895 -DsliceValueMin=0 -DsliceValueMax=2         	-DcropOffsetX=334 -DcropOffsetY=6 -DcropOffsetZ=284 -Dscale=1 		-- --no-splash /projects/pilot_spim/Christopher/pipeline/jobs_master_2.1/hdf5//export.bsh
